{
  "rotationDuration": 5000,
  "transitionAnimation": "fadeSlide",
  "colors": {
    "panelBackground": "#F3F3F3",
    "panelBorder": "#002740",
    "primaryText": "#002740",
    "secondaryText": "#666666",
    "progressBarBackground": "#E0E0E0",
    "progressBarFill": "#002740",
    "progressBarFillEnd": "#004d73",
    "accentColor": "#9146ff"
  },
  "youtube": {
    "apiKey": "",
    "channelId": ""
  },
  "elements": [
    {
      "id": "followerGoal",
      "type": "progress",
      "enabled": true,
      "title": "Follower Goal",
      "emote": "follower.png",
      "zIndex": 1,
      "animation": "fadeIn",
      "dataSource": "twitch.followers",
      "fields": {
        "goal": 200,
        "current": 0,
        "showPercentage": true,
        "subtext": ""
      }
    },
    {
      "id": "subscriberGoal",
      "type": "progress",
      "enabled": true,
      "title": "Subscriber Goal",
      "emote": "subscriber.png",
      "zIndex": 2,
      "animation": "slideLeft",
      "dataSource": "twitch.subscribers",
      "fields": {
        "goal": 500,
        "current": 0,
        "showPercentage": true,
        "subtext": ""
      }
    },
    {
      "id": "donations",
      "type": "counter",
      "enabled": true,
      "title": "Total Donations",
      "emote": "donation.png",
      "zIndex": 3,
      "animation": "slideUp",
      "dataSource": "custom.donations",
      "fields": {
        "value": 0,
        "prefix": "$",
        "suffix": "",
        "subtext": "Thank you for your support!"
      }
    },
    {
      "id": "chatCommands",
      "type": "list",
      "enabled": true,
      "title": "Chat Commands",
      "emote": "",
      "zIndex": 4,
      "animation": "fadeIn",
      "dataSource": "none",
      "fields": {
        "maxItemsToShow": 3,
        "showAsCarousel": true,
        "subtext": "",
        "items": [
          {
            "name": "!discord",
            "description": "Join our Discord server",
            "emote": "discord.png"
          },
          {
            "name": "!vod",
            "description": "Watch latest stream",
            "emote": "vod.png"
          }
        ]
      }
    },
    {
      "id": "latestVOD",
      "type": "info",
      "enabled": true,
      "title": "Latest VOD",
      "emote": "ðŸŽ¬",
      "zIndex": 5,
      "animation": "scale",
      "dataSource": "twitch.vods",
      "fields": {
        "showThumbnail": false,
        "text": "Latest Stream Highlights",
        "subtext": "Type !vod or !youtube in chat"
      }
    }
  ]
}
